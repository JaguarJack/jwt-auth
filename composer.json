{
    "name": "catchadmin/jwt-auth",
    "description": "catch-admin jwt authï¼Œyou can use in thinkphp6.0.*",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "lcobucci/jwt": "^4.1.5",
        "topthink/framework": ">=6.0.10"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "JaguarJack",
            "email": "njphper@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "catchAdmin\\jwt\\": "src"
        }
    },
    "extra": {
        "think": {
            "services": [
                "catchAdmin\\jwt\\JwtService"
            ],
            "config": {
                "jwt": "config/jwt.php"
            }
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "@php think jwt:create"
        ]
    },

    "minimum-stability": "dev"
}
